{
  "githubhook": {
    "host": "0.0.0.0",
    "port": 9000,
    "branch": "refs/heads/production",
    "path": "/event"
  },
  "express": {
    "host": "0.0.0.0",
    "port": 9001
  },
  "socket": {
    "path": "/realtime"
  },
  "deployments": [
    {
      "name": "digibotv2",
      "path": "/home/mc/rdelrod/digibotv2",
      "main": "index.js"
    },
    {
      "name": "mc-api",
      "pm2": {
        "opts": {
          "exec_mode": "cluster",
          "instances": 4
        }
      },
      "path": "/home/mc//rdelrod/mc-api",
      "main": "index.js"
    },
    {
      "name": "mcfd",
      "path": "/home/mc/rdelrod/mcfd",
      "main": "index.js"
    }
  ],
  "global": {
    "post": [
      "npm install --verbose"
    ]
  },
  "listeners": [
    {
      "type": "webhook",
      "on": [
        "deploy"
      ],
      "uri": "http://127.0.0.1:8302/event"
    }
  ]
}
